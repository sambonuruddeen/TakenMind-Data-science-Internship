import numpy as np

my_list1 = [1, 2, 3, 4]
my_list2 = [5, 6, 7, 8]

#my_array1 = np.array([my_list1])

my_array = np.array([my_list1,my_list2])
#print my_array

#shape
#print my_array.shape

#datatype
#print my_array.dtype

#zeros, ones, empty, eye, arange
new_array1 = np.zeros(5)
#print new_array1

new_array2 = np.ones([5,5])
#print new_array2

new_array3 = np.empty([5])
#print new_array3

new_array4 = np.eye(5)
print new_array4

new_array5 = np.arange(5, 50, 3)
#print new_array5


